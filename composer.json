{
    "name": "morrislaptop/symfony-custom-normalizers",
    "description": "Normalizers for common classes around the place",
    "keywords": [
        "morrislaptop",
        "symfony-custom-normalizers"
    ],
    "homepage": "https://github.com/morrislaptop/symfony-custom-normalizers",
    "license": "MIT",
    "authors": [
        {
            "name": "Craig Morris",
            "email": "craig.michael.morris@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "symfony/serializer": "^5.2"
    },
    "require-dev": {
        "brick/date-time": "^0.2.3",
        "friendsofphp/php-cs-fixer": "^2.17",
        "nesbot/carbon": "^2.46",
        "phpunit/phpunit": "^9.5",
        "spatie/ray": "^1.10",
        "symfony/property-access": "^5.2",
        "vimeo/psalm": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Morrislaptop\\SymfonyCustomNormalizers\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Morrislaptop\\SymfonyCustomNormalizers\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
